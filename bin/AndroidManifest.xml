<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beygdu"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BeygingarActivity"
            android:label="@string/title_activity_beygingar" >
        </activity>
        <activity
            android:name=".TableTest"
            android:label="@string/title_activity_table_test" >
        </activity>
        <activity
            android:name=".AboutActivity"
            android:label="@string/title_activity_about"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.beygdu.MainActivity" />
        </activity>
        <!-- activity
            android:name=".JsonActivity"
            android:label="@string/title_activity_json" >
        </activity-->
        <activity
		      android:name=".JsonActivity"
		      android:label="@string/app_name" >
		      <intent-filter>
		         <action android:name="android.intent.action.MAIN" />
		
		         <category android:name="android.intent.category.LAUNCHER" />
		      </intent-filter>
		   </activity>
     </application>

</manifest>
